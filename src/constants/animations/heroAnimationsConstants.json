{
  "CREATIVE_DEVELOPER": {
    "SELECTOR": "#creative_developer",
    "PARAMS": {
      "TRANSFORM": "translateY(75px) scaleY(0.25) skew(-45deg)",
      "OPACITY": 0,
      "FILTER": "blur(80px)",
      "DELAY": 1,
      "DURATION": 1,
      "EASE": "power3.out"
    }
  },
  "MOCO": {
    "SELECTOR": "#moco",
    "PARAMS": {
      "TRANSFORM": "translateY(100px) scaleY(0.75) skew(-15deg)",
      "OPACITY": 0,
      "FILTER": "blur(100px)",
      "DURATION": 1,
      "EASE": "power3.out"
    },
    "POSITION_PARAMETER": "-=0.75"
  },
  "FEDERICO_DESMOULIN_2024": {
    "SELECTOR": "#federico_desmoulin_2024",
    "PARAMS": {
      "TRANSFORM": "translateY(100px)",
      "OPACITY": 0,
      "DURATION": 1,
      "EASE": "power3.out"
    },
    "POSITION_PARAMETER": "-=0.2"
  },
  "SCROLL_DOWN": {
    "SELECTOR": "#scroll_down",
    "PARAMS": {
      "TRANSFORM": "translateY(100px)",
      "OPACITY": 0,
      "DURATION": 1,
      "EASE": "power3.out"
    },
    "POSITION_PARAMETER": "-=0.8"
  },
  "SUN": {
    "SELECTOR": "#sun",
    "FROM": {
      "PARAMS": {
        "TRANSFORM": "translateY(-500px)",
        "OPACITY": 0,
        "DURATION": 3,
        "EASE": "power3.out"
      },
      "POSITION_PARAMETER": "-=1.8"
    },
    "TO": {
      "TRANSFORM": "translateX(50vw)",
      "SCROLL_TRIGGER": {
        "TRIGGER": "#hero_slice",
        "SCROLLER": "body",
        "START": "top top",
        "SCRUB": true
      }
    }
  }
}

{
  "defaultValues": {
    "name": "lightning_component",
    "visible": true,
    "scale": 2.5,
    "material": {
      "color": "#fff",
      "transparent": true,
      "opacity": 0,
      "emissive": "#fff",
      "emissiveIntensity": 0
    }
  },
  "animations": {
    "material": {
      "keyframes": {
        "step_1": {
          "opacity": 0.15,
          "emissiveIntensity": 60
        },
        "step_2": {
          "opacity": 0.1,
          "emissiveIntensity": 40
        },
        "step_3": {
          "opacity": 0,
          "emissiveIntensity": 0
        },
        "step_4": {
          "opacity": 0.125,
          "emissiveIntensity": 50
        },
        "step_5": {
          "opacity": 0.05,
          "emissiveIntensity": 20
        },
        "step_10": {
          "opacity": 0,
          "emissiveIntensity": 0
        },
        "easeEach": "none"
      }
    }
  }
}
